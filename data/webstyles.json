{
  "variables": {
    "$colors": {
      "primary": "#2563eb",
      "secondary": "#1e40af",
      "text": "#1f2937",
      "lightText": "#6b7280",
      "background": "#ffffff",
      "accent": "#3b82f6",
      "success": "#10b981",
      "warning": "#f59e0b",
      "error": "#ef4444",
      "border": "#e5e7eb",
      "backgroundAlt": "#f8fafc"
    },
    "$typography": {
      "fontFamily": "Inter, system-ui, -apple-system, sans-serif",
      "sizes": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem",
        "5xl": "3rem",
        "6xl": "4rem"
      },
      "weights": {
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700",
        "extrabold": "800"
      },
      "lineHeights": {
        "tight": "1.25",
        "normal": "1.5",
        "relaxed": "1.75"
      }
    },
    "$spacing": {
      "section": {
        "paddingY": "5rem",
        "container": "1280px"
      },
      "grid": {
        "gap": "2rem"
      }
    },
    "$animation": {
      "duration": "0.3s",
      "timing": "ease-in-out"
    },
    "$borderRadius": {
      "sm": "0.25rem",
      "md": "0.375rem",
      "lg": "0.5rem",
      "xl": "1rem",
      "full": "9999px"
    },
    "$shadows": {
      "sm": "0 1px 2px rgba(0, 0, 0, 0.05)",
      "md": "0 4px 6px rgba(0, 0, 0, 0.1)",
      "lg": "0 10px 15px rgba(0, 0, 0, 0.1)",
      "xl": "0 20px 25px rgba(0, 0, 0, 0.15)"
    }
  },

  ":root": {
    "--primary-color": "var(--colors-primary)",
    "--secondary-color": "var(--colors-secondary)",
    "--text-color": "var(--colors-text)",
    "--light-text-color": "var(--colors-lightText)",
    "--background-color": "var(--colors-background)",
    "--accent-color": "var(--colors-accent)",
    "--success-color": "var(--colors-success)",
    "--warning-color": "var(--colors-warning)",
    "--error-color": "var(--colors-error)",
    "--border-color": "var(--colors-border)",
    "--background-alt-color": "var(--colors-backgroundAlt)",
    "--font-family": "var(--typography-fontFamily)",
    "--font-size-xs": "var(--typography-sizes-xs)",
    "--font-size-sm": "var(--typography-sizes-sm)",
    "--font-size-base": "var(--typography-sizes-base)",
    "--font-size-lg": "var(--typography-sizes-lg)",
    "--font-size-xl": "var(--typography-sizes-xl)",
    "--font-size-2xl": "var(--typography-sizes-2xl)",
    "--font-size-3xl": "var(--typography-sizes-3xl)",
    "--font-size-4xl": "var(--typography-sizes-4xl)",
    "--font-size-5xl": "var(--typography-sizes-5xl)",
    "--font-size-6xl": "var(--typography-sizes-6xl)",
    "--font-weight-normal": "var(--typography-weights-normal)",
    "--font-weight-medium": "var(--typography-weights-medium)",
    "--font-weight-semibold": "var(--typography-weights-semibold)",
    "--font-weight-bold": "var(--typography-weights-bold)",
    "--font-weight-extrabold": "var(--typography-weights-extrabold)",
    "--line-height-tight": "var(--typography-lineHeights-tight)",
    "--line-height-normal": "var(--typography-lineHeights-normal)",
    "--line-height-relaxed": "var(--typography-lineHeights-relaxed)",
    "--section-padding-y": "var(--spacing-section-paddingY)",
    "--container-width": "var(--spacing-section-container)",
    "--grid-gap": "var(--spacing-grid-gap)",
    "--animation-duration": "var(--animation-duration)",
    "--animation-timing": "var(--animation-timing)",
    "--border-radius-sm": "var(--borderRadius-sm)",
    "--border-radius-md": "var(--borderRadius-md)",
    "--border-radius-lg": "var(--borderRadius-lg)",
    "--border-radius-xl": "var(--borderRadius-xl)",
    "--border-radius-full": "var(--borderRadius-full)",
    "--shadow-sm": "var(--shadows-sm)",
    "--shadow-md": "var(--shadows-md)",
    "--shadow-lg": "var(--shadows-lg)",
    "--shadow-xl": "var(--shadows-xl)"
  },

  "*": {
    "margin": "0",
    "padding": "0",
    "box-sizing": "border-box"
  },

  "body": {
    "font-family": "var(--font-family)",
    "color": "var(--text-color)",
    "line-height": "var(--line-height-normal)",
    "background-color": "var(--background-color)"
  },

  "components": {
    ".navbar": {
      "position": "fixed",
      "top": "0",
      "left": "0",
      "right": "0",
      "background": "rgba(255, 255, 255, 0.9)",
      "backdrop-filter": "blur(10px)",
      "z-index": "1000",
      "padding": "1rem 0",
      "box-shadow": "var(--shadow-sm)",
      ".container": {
        "max-width": "var(--container-width)",
        "margin": "0 auto",
        "padding": "0 1rem",
        "display": "flex",
        "align-items": "center",
        "justify-content": "space-between"
      },
      ".logo": {
        "font-size": "var(--font-size-2xl)",
        "font-weight": "var(--font-weight-bold)",
        "color": "var(--primary-color)",
        "text-decoration": "none"
      },
      ".nav-links": {
        "display": "flex",
        "gap": "2rem",
        "a": {
          "color": "var(--text-color)",
          "text-decoration": "none",
          "font-weight": "var(--font-weight-medium)",
          "transition": "color var(--animation-duration) var(--animation-timing)",
          "&:hover": {
            "color": "var(--primary-color)"
          }
        }
      }
    },
    ".hero": {
      "padding-top": "7rem",
      "min-height": "100vh",
      "background-size": "cover",
      "background-position": "center",
      "position": "relative",
      "&::before": {
        "content": "''",
        "position": "absolute",
        "top": "0",
        "left": "0",
        "right": "0",
        "bottom": "0",
        "background": "rgba(0, 0, 0, 0.5)"
      },
      ".container": {
        "position": "relative",
        "color": "white",
        "padding-top": "8rem",
        "text-align": "center",
        "max-width": "var(--container-width)",
        "margin": "0 auto",
        "padding": "0 1rem",
        "h1": {
          "font-size": "var(--font-size-6xl)",
          "font-weight": "var(--font-weight-extrabold)",
          "margin-bottom": "1.5rem",
          "line-height": "var(--line-height-tight)"
        },
        "p": {
          "font-size": "var(--font-size-xl)",
          "margin-bottom": "2rem",
          "max-width": "600px",
          "margin-left": "auto",
          "margin-right": "auto"
        },
        ".cta-buttons": {
          "display": "flex",
          "gap": "1rem",
          "justify-content": "center",
          ".button": {
            "padding": "1rem 2rem",
            "border-radius": "var(--border-radius-lg)",
            "font-weight": "var(--font-weight-semibold)",
            "text-decoration": "none",
            "transition": "transform var(--animation-duration) var(--animation-timing)",
            "&:hover": {
              "transform": "translateY(-2px)"
            },
            "&.primary": {
              "background": "var(--primary-color)",
              "color": "white"
            },
            "&.secondary": {
              "background": "rgba(255, 255, 255, 0.1)",
              "color": "white",
              "border": "2px solid white"
            }
          }
        }
      }
    },
    ".about": {
      "padding": "var(--section-padding-y) 0",
      ".container": {
        "max-width": "var(--container-width)",
        "margin": "0 auto",
        "padding": "0 1rem",
        ".section-title": {
          "text-align": "center",
          "margin-bottom": "3rem",
          "h2": {
            "font-size": "var(--font-size-4xl)",
            "font-weight": "var(--font-weight-bold)",
            "color": "var(--text-color)",
            "margin-bottom": "1rem"
          },
          "p": {
            "font-size": "var(--font-size-lg)",
            "color": "var(--light-text-color)"
          }
        },
        ".content": {
          "display": "grid",
          "grid-template-columns": "1fr 1fr",
          "gap": "var(--grid-gap)",
          "align-items": "center",
          "img": {
            "width": "100%",
            "border-radius": "var(--border-radius-xl)"
          },
          ".stats": {
            "display": "grid",
            "grid-template-columns": "repeat(2, 1fr)",
            "gap": "var(--grid-gap)",
            "margin-top": "2rem",
            ".stat": {
              "text-align": "center",
              ".value": {
                "font-size": "var(--font-size-4xl)",
                "font-weight": "var(--font-weight-bold)",
                "color": "var(--primary-color)"
              },
              ".label": {
                "color": "var(--light-text-color)"
              }
            }
          }
        }
      }
    },
    ".services": {
      "padding": "var(--section-padding-y) 0",
      "background": "var(--background-alt-color)",
      ".container": {
        "max-width": "var(--container-width)",
        "margin": "0 auto",
        "padding": "0 1rem",
        ".services-grid": {
          "display": "grid",
          "grid-template-columns": "repeat(auto-fit, minmax(250px, 1fr))",
          "gap": "var(--grid-gap)",
          ".service-card": {
            "background": "white",
            "padding": "2rem",
            "border-radius": "var(--border-radius-lg)",
            "box-shadow": "var(--shadow-md)",
            "transition": "transform var(--animation-duration) var(--animation-timing)",
            "&:hover": {
              "transform": "translateY(-5px)"
            },
            ".icon": {
              "color": "var(--primary-color)",
              "font-size": "2rem",
              "margin-bottom": "1rem"
            },
            "h3": {
              "margin-bottom": "0.5rem",
              "font-size": "var(--font-size-xl)",
              "font-weight": "var(--font-weight-semibold)"
            },
            "p": {
              "color": "var(--light-text-color)"
            }
          }
        }
      }
    },
    ".team": {
      "padding": "var(--section-padding-y) 0",
      ".team-grid": {
        "display": "grid",
        "grid-template-columns": "repeat(auto-fit, minmax(250px, 1fr))",
        "gap": "var(--grid-gap)",
        ".team-member": {
          "text-align": "center",
          "img": {
            "width": "200px",
            "height": "200px",
            "border-radius": "var(--border-radius-full)",
            "object-fit": "cover",
            "margin-bottom": "1rem"
          },
          "h3": {
            "font-size": "var(--font-size-xl)",
            "font-weight": "var(--font-weight-semibold)",
            "margin-bottom": "0.5rem"
          },
          ".role": {
            "color": "var(--primary-color)",
            "font-weight": "var(--font-weight-medium)",
            "margin-bottom": "0.5rem"
          },
          ".bio": {
            "color": "var(--light-text-color)"
          }
        }
      }
    },
    ".contact": {
      "padding": "var(--section-padding-y) 0",
      "background": "var(--background-alt-color)",
      ".container": {
        "max-width": "var(--container-width)",
        "margin": "0 auto",
        "padding": "0 1rem",
        ".section-title": {
          "text-align": "center",
          "margin-bottom": "3rem",
          "h2": {
            "font-size": "var(--font-size-4xl)",
            "font-weight": "var(--font-weight-bold)",
            "color": "var(--text-color)",
            "margin-bottom": "1rem"
          },
          "p": {
            "font-size": "var(--font-size-lg)",
            "color": "var(--light-text-color)"
          }
        },
        ".contact-content": {
          "display": "grid",
          "grid-template-columns": "1fr",
          "gap": "var(--grid-gap)",
          "max-width": "600px",
          "margin": "0 auto",
          ".contact-info": {
            ".info-item": {
              "background": "white",
              "padding": "1.5rem",
              "border-radius": "var(--border-radius-lg)",
              "margin-bottom": "1rem",
              "box-shadow": "var(--shadow-sm)",
              "h4": {
                "font-size": "var(--font-size-lg)",
                "font-weight": "var(--font-weight-semibold)",
                "color": "var(--text-color)",
                "margin-bottom": "0.5rem"
              },
              "p": {
                "color": "var(--light-text-color)",
                "font-size": "var(--font-size-base)"
              }
            },
            ".social-links": {
              "display": "flex",
              "gap": "1rem",
              "justify-content": "center",
              "margin-top": "2rem",
              "a": {
                "display": "flex",
                "align-items": "center",
                "justify-content": "center",
                "width": "40px",
                "height": "40px",
                "border-radius": "var(--border-radius-full)",
                "background": "white",
                "color": "var(--primary-color)",
                "box-shadow": "var(--shadow-sm)",
                "transition": "transform var(--animation-duration) var(--animation-timing)",
                "&:hover": {
                  "transform": "translateY(-2px)",
                  "color": "var(--primary-color)"
                },
                "svg": {
                  "width": "20px",
                  "height": "20px"
                }
              }
            }
          }
        }
      }
    },

    ".footer": {
        "padding": "2rem 0",
        "background": "var(--text-color)",
        "color": "white",
        ".container": {
          "max-width": "var(--container-width)",
          "margin": "0 auto",
          "padding": "0 1rem",
          "display": "flex",
          "justify-content": "space-between",
          "align-items": "center",
          ".footer-links": {
            "display": "flex",
            "gap": "2rem",
            "a": {
              "color": "rgba(255, 255, 255, 0.8)",
              "text-decoration": "none",
              "transition": "color var(--animation-duration) var(--animation-timing)",
              "&:hover": {
                "color": "white"
              }
            }
          }
        }
      }
  },

  "mediaQueries": {
    "(max-width: 768px)": {
      "navbar": {
        ".nav-links": {
          "display": "none"
        }
      },
      "hero": {
        ".container": {
          "h1": {
            "font-size": "var(--font-size-4xl)"
          }
        }
      }
    }
  }
}
